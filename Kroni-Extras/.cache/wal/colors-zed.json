{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#15091590",
                "editor.background": "#15091590",
                "editor.foreground": "#c4c1c4",
                "text": "#c4c1c4",
                "text.muted": "#c4c1c470",
                "text.ignored": "#c4c1c440",
                "text.placeholder": "#c4c1c450",
                "ignored": "#c4c1c430",
                "element.hover": "#c4c1c430",
                "ghost_element.hover": "#38266730",
                "ghost_element.selected": "#38266730",
                "ghost_element.active": "#38266760",
                "border": "#150915",
                "editor.highlighted_line.background": "#38266710",
                "editor.active_line.background": "#38266710",
                "panel.background": "#15091590",
                "title_bar.background": "#15091590",
                "title_bar.inactive_background": "#15091590",
                "status_bar.background": "#15091590",
                "drop_target.background": "#15091590",
                "elevated_surface.background": "#150915",
                "toolbar.background": "#15091590",
                "tab_bar.background": "#15091590",
                "tab.inactive_background": "#15091590",
                "tab.active_background": "#38266730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#150915",
                "scrollbar.thumb.background": "#382667",
                "editor.gutter.background": "#15091590",
                "terminal.background": "#15091510",
                "terminal.foreground": "#c4c1c4",
                "terminal.dim_foreground": "#c4c1c4",
                "terminal.bright_foreground": "#c4c1c4",
                "terminal.ansi.black": "#150915",
                "terminal.ansi.red": "#382667",
                "terminal.ansi.green": "#4B315F",
                "terminal.ansi.yellow": "#624D69",
                "terminal.ansi.blue": "#856671",
                "terminal.ansi.magenta": "#5B3887",
                "terminal.ansi.cyan": "#6A5395",
                "terminal.ansi.white": "#c4c1c4",
                "terminal.ansi.bright_black": "#6c586c",
                "terminal.ansi.bright_red": "#382667",
                "terminal.ansi.bright_green": "#4B315F",
                "terminal.ansi.bright_yellow": "#624D69",
                "terminal.ansi.bright_blue": "#856671",
                "terminal.ansi.bright_magenta": "#5B3887",
                "terminal.ansi.bright_cyan": "#6A5395",
                "terminal.ansi.bright_white": "#c4c1c4",
                "modified": "#382667",
                "syntax": {
                    "attribute": {
                        "color": "#382667"
                    },
                    "boolean": {
                      "color": "#4B315F"
                    },
                    "comment": {
                      "color": "#6c586c"
                    },
                    "comment.doc": {
                      "color": "#6c586c"
                    },
                    "constant": {
                      "color": "#4B315F"
                    },
                    "function": {
                      "color": "#6A5395"
                    },
                    "keyword": {
                      "color": "#856671"
                    },
                    "number": {
                      "color": "#5B3887"
                    },
                    "operator": {
                      "color": "#856671"
                    },
                    "string": {
                      "color": "#382667"
                    },
                    "variable": {
                      "color": "#4B315F"
                    }
                },
                "players": [
                  {
                    "cursor": "#382667",
                    "background": "#150915",
                    "selection": "#38266730"
                  }
                ]
            }
        }
    ]
}
