{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0e132190",
                "editor.background": "#0e132190",
                "editor.foreground": "#c2c4c7",
                "text": "#c2c4c7",
                "text.muted": "#c2c4c770",
                "text.ignored": "#c2c4c740",
                "text.placeholder": "#c2c4c750",
                "ignored": "#c2c4c730",
                "element.hover": "#c2c4c730",
                "ghost_element.hover": "#4B4B5430",
                "ghost_element.selected": "#4B4B5430",
                "ghost_element.active": "#4B4B5460",
                "border": "#0e1321",
                "editor.highlighted_line.background": "#4B4B5410",
                "editor.active_line.background": "#4B4B5410",
                "panel.background": "#0e132190",
                "title_bar.background": "#0e132190",
                "title_bar.inactive_background": "#0e132190",
                "status_bar.background": "#0e132190",
                "drop_target.background": "#0e132190",
                "elevated_surface.background": "#0e1321",
                "toolbar.background": "#0e132190",
                "tab_bar.background": "#0e132190",
                "tab.inactive_background": "#0e132190",
                "tab.active_background": "#4B4B5430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0e1321",
                "scrollbar.thumb.background": "#4B4B54",
                "editor.gutter.background": "#0e132190",
                "terminal.background": "#0e132110",
                "terminal.foreground": "#c2c4c7",
                "terminal.dim_foreground": "#c2c4c7",
                "terminal.bright_foreground": "#c2c4c7",
                "terminal.ansi.black": "#0e1321",
                "terminal.ansi.red": "#4B4B54",
                "terminal.ansi.green": "#61606C",
                "terminal.ansi.yellow": "#3C5780",
                "terminal.ansi.blue": "#727489",
                "terminal.ansi.magenta": "#80818E",
                "terminal.ansi.cyan": "#9896A4",
                "terminal.ansi.white": "#c2c4c7",
                "terminal.ansi.bright_black": "#5d6272",
                "terminal.ansi.bright_red": "#4B4B54",
                "terminal.ansi.bright_green": "#61606C",
                "terminal.ansi.bright_yellow": "#3C5780",
                "terminal.ansi.bright_blue": "#727489",
                "terminal.ansi.bright_magenta": "#80818E",
                "terminal.ansi.bright_cyan": "#9896A4",
                "terminal.ansi.bright_white": "#c2c4c7",
                "modified": "#4B4B54",
                "syntax": {
                    "attribute": {
                        "color": "#4B4B54"
                    },
                    "boolean": {
                      "color": "#61606C"
                    },
                    "comment": {
                      "color": "#5d6272"
                    },
                    "comment.doc": {
                      "color": "#5d6272"
                    },
                    "constant": {
                      "color": "#61606C"
                    },
                    "function": {
                      "color": "#9896A4"
                    },
                    "keyword": {
                      "color": "#727489"
                    },
                    "number": {
                      "color": "#80818E"
                    },
                    "operator": {
                      "color": "#727489"
                    },
                    "string": {
                      "color": "#4B4B54"
                    },
                    "variable": {
                      "color": "#61606C"
                    }
                },
                "players": [
                  {
                    "cursor": "#4B4B54",
                    "background": "#0e1321",
                    "selection": "#4B4B5430"
                  }
                ]
            }
        }
    ]
}
